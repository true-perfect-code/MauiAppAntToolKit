<div>ANT Select</div>
<br />
<br />
<AntDesign.SimpleSelect Size="small">
    <SelectOptions>
        <AntDesign.SimpleSelectOption Value="VO" Label="Volvo"></AntDesign.SimpleSelectOption>
        <AntDesign.SimpleSelectOption Value="SA" Label="Saab"></AntDesign.SimpleSelectOption>
        <AntDesign.SimpleSelectOption Value="OP" Label="Opel"></AntDesign.SimpleSelectOption>
        <AntDesign.SimpleSelectOption Value="AU" Label="Audi"></AntDesign.SimpleSelectOption>
